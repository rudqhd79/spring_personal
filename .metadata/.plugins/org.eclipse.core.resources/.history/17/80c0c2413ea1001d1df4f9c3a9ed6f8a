<script language="JavaScript">
	function jsChangeOrder() {
		var form = document.mainForm;
		var se = document.getElementsByName("mgr");
		var rank1;
		var rank2;

		for (j = 0; j < se.length; j++) {
			rank1 = form.mgrRank[j].value;
			if ("" == rank1) {
				window.alert("게시판명을 입력하세요.");
				return false;
			}

			if (se.length > j + 1) {
				for (i = (j + 1); i < 4; i++) {
					rank2 = form.mgrRank[i].value;
					if (rank1 == rank2) {
						alert('중복된값이 있습니다.');
						//                        return false;
					}
				}
			}
		}

		//        form.action = "";
		//        form.submit();
	}
</script>
